<?xml version="1.0" encoding="utf-8" ?>
<Shell x:Class="HealthMaui.AppShell"
       xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
       xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
       xmlns:pages="clr-namespace:HealthMaui.Pages">

    <ShellContent Title="Patients"
                  ContentTemplate="{DataTemplate pages:PatientsPage}"
                  Route="Patients" />
   <ShellContent Title="Physicians"
              ContentTemplate="{DataTemplate pages:PhysiciansPage}"
              Route="Physicians" />
   <ShellContent Title="Appointments"
              ContentTemplate="{DataTemplate pages:AppointmentsPage}"
              Route="Appointments" />

</Shell>
